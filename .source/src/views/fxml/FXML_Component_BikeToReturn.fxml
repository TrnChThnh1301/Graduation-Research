<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="165.0" prefWidth="415.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="bikeImage" fitHeight="125.0" fitWidth="125.0" layoutX="28.0" layoutY="25.0" pickOnBounds="true">
            <image>
                <Image url="@../../assets/img/docks/dongmac.jpg" />
            </image>
        </ImageView>
        <Label fx:id="bikeName" layoutX="189.0" layoutY="26.0" text="Bike X">
            <font>
                <Font name="Segoe UI Bold" size="20.0" />
            </font>
        </Label>
      <Label fx:id="bikeType" layoutX="281.0" layoutY="57.0" text="Mock Type">
         <font>
            <Font name="Segoe UI" size="16.0" />
         </font>
      </Label>
        <Label layoutX="189.0" layoutY="58.0" text="Type">
            <font>
                <Font name="Segoe UI Bold" size="16.0" />
            </font>
        </Label>
      <Button fx:id="returnBtn" layoutX="191.0" layoutY="108.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="100.0" text="RETURN">
         <font>
            <Font size="16.0" />
         </font></Button>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="20.0" top="20.0" />
    </padding>
</AnchorPane>
