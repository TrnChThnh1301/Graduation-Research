<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="172.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="bikeImage" fitHeight="131.0" fitWidth="131.0" layoutX="30.0" layoutY="40.0" pickOnBounds="true">
            <image>
                <Image url="@../../assets/img/docks/dongmac.jpg" />
            </image>
        </ImageView>
        <Label fx:id="bikeName" layoutX="182.0" layoutY="35.0" text="MOCK_BIKE_NAME">
            <font>
                <Font name="Segoe UI Bold" size="20.0" />
            </font>
        </Label>
        <Label layoutX="183.0" layoutY="69.0" text="Type">
            <font>
                <Font name="Segoe UI Bold" size="16.0" />
            </font>
        </Label>
      <Label fx:id="bikeType" layoutX="280.0" layoutY="69.0" text="MOCK_TYPE">
         <font>
            <Font name="Segoe UI" size="16.0" />
         </font>
      </Label>
        <Label layoutX="182.0" layoutY="96.0" text="Distance">
            <font>
                <Font name="Segoe UI Bold" size="16.0" />
            </font>
        </Label>
      <Label fx:id="distanceEstimation" layoutX="280.0" layoutY="96.0" prefHeight="19.0" prefWidth="60.0" text="0 km">
         <font>
            <Font name="Segoe UI" size="16.0" />
         </font>
      </Label>
      <Button fx:id="viewBikeButton" layoutX="182.0" layoutY="129.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="100.0" text="DETAIL">
         <font>
            <Font size="16.0" />
         </font></Button>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="20.0" top="20.0" />
    </padding>
</AnchorPane>
